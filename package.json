{
  "name": "smooth-page-scroll",
  "version": "1.0.5",
  "description": "In page smooth scroll",
  "main": "./src/smooth-page-scroll",
  "scripts": {
    "build": "npm run bundle && npm run min",
    "bundle": "browserify ./src/index.js -o ./dist/smooth-page-scroll.js",
    "min": "uglifyjs ./dist/smooth-page-scroll.js > ./dist/smooth-page-scroll.min.js",
    "_dev": "../node_modules/watchify/bin/cmd.js ./src/index.js -o ./dist/smooth-page-scroll.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leader22/smooth-page-scroll.git"
  },
  "keywords": [
    "smooth",
    "scroll",
    "smooth-scroll"
  ],
  "author": "leader22",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leader22/smooth-page-scroll/issues"
  },
  "homepage": "https://github.com/leader22/smooth-page-scroll#readme",
  "devDependencies": {
    "browserify": "^13.1.0",
    "uglifyify": "^3.0.3"
  }
}
